"TableView": {
	scrollsToTop: false
},
"WebView": {
	scrollsToTop: false
},
"TextArea": {
	scrollsToTop: false
},
"ScrollView": {
	scrollsToTop: false
},
"Window[platform=android]": {
	windowSoftInputMode: Ti.UI.Android.SOFT_INPUT_ADJUST_PAN,
	softKeyboardOnFocus: Ti.UI.Android.SOFT_KEYBOARD_SHOW_ON_FOCUS,
	fullscreen: true,
	navBarHidden: true,
	width: "100%",
	height: "100%"
},
"ScrollView[platform=android]": {
	overScrollMode: Ti.UI.Android.OVER_SCROLL_IF_CONTENT_SCROLLS
}